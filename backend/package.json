{
  "name": "stylehub-backend",
  "version": "1.0.0",
  "description": "Style Hub - Fashion Marketplace Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:create": "mysql -u root -p < ../database/schema.sql",
    "db:seed": "mysql -u root -p < ../database/seed.sql",
    "db:reset": "npm run db:create && npm run db:seed"
  },
  "keywords": [
    "fashion",
    "ecommerce",
    "marketplace"
  ],
  "author": "Style Hub Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "axios": "^1.13.2",
    "nodemon": "^3.0.2"
  }
}
